{
  "languageserver": {
    "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
            "filetypes": ["go"]
    },
    "clangd": {
            "command": "clangd",
            "args": ["--background-index", "--clang-tidy", "--completion-style=detailed", "--fallback-style=google", "--header-insertion=iwyu"],

            "rootPatterns": ["compile_flags.txt", "compile_commands.json"],
            "filetypes": ["c", "cpp"]
    },
  },
  "clangd.enabled": true,
  "clangd.semanticHighlighting": true,
  "coc.preferences.colorSupport": true,
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "css", "markdown"],
  "coc.source.file.shortcut": "F",
  "coc.source.file.priority": 89,
  "coc.source.file.ignoreHidden": false,
  "coc.source.around.shortcut": "A",
  "coc.source.around.priority": 88,
  "coc.source.buffer.shortcut": "B",
  "coc.source.buffer.priority": 87,
  "coc.source.buffer.ignoreGitignore": false,
  "coc.source.syntax.shortcut": "X",
  "coc.source.syntax.priority": 79,
  "coc.source.tag.shortcut": "T",
  "coc.source.tag.priority": 78,
  "coc.source.dictionary.shortcut": "D",
  "coc.source.dictionary.priority": 9,
  "coc.source.dictionary.filetypes": ["markdown", "text", "help", "gitcommit", "snippets"],
  "coc.source.emoji.shortcut": "E",
  "coc.source.emoji.priority": 8,
  "coc.source.emoji.filetypes": ["markdown", "text"],
  "diagnostic.displayByAle": true,
  "list.source.files.args": ["--hidden", "--files"],
  "list.source.files.defaultOptions": ["--number-select"],
  "prettier.eslintIntegration": true,
  "prettier.printWidth": 120,
  "prettier.tabWidth": 4,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "prettier.parse": "flow",
  "python.jediEnabled": true,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.autoTrigger": "trigger",
  "suggest.minTriggerInputLength": 2,
  "suggest.snippetIndicator": "â–º",
  "suggest.timeout": 700,
  "suggest.triggerAfterInsertEnter": true,
}
